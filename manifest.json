{
  "name": "ABCD extension",
  "description": "Bookmark the pages",
  "version": "1.1",
  "icons": {
    "16": "images/Mascot.png",
    "32": "images/icon (4).png",
    "48": "images/App icon.png",
    "128": "images/App icon.png"
  },
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "bookmarks",
    "storage",
    "contextMenus",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "beforeSignin/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ]
}
